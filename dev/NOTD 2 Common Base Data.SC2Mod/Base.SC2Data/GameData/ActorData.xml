        <Host Subject="_Unit"/>
        <HostSiteOps Ops="SOpAttachRandomDamage"/>
        <Model value="ShortCircuitImpact"/>
        <AutoScaleFromSelectionFactor value="1.000000"/>
    </CActorModel>
    <CActorModel id="Stunned" parent="ModelAnimationStyleContinuous">
        <Inherits index="CloakEffect" value="1"/>
        <Inherits index="Decal" value="1"/>
        <Inherits index="Visibility" value="1"/>
        <On Terms="ActorCreation" Send="AnimBracketStart GL_BSD GLbirth GLstand GLdeath"/>
        <On Terms="Behavior.BonebreakerStunned.On" Send="Create"/>
        <On Terms="ActorCreation" Send="AnimBracketStart $BSD"/>
        <On Terms="Behavior.BonebreakerStunned.Off" Send="AnimBracketStop BSD"/>
        <On Terms="Behavior.BansheeScreamStun.On" Send="Create"/>
        <On Terms="Behavior.BansheeScreamStun.Off" Send="Destroy"/>
        <Host Subject="_Selectable"/>
        <HostSiteOps Ops="SOpAttachOverhead"/>
        <Model value="Stun"/>
        <AutoScaleFactor value="1.000000"/>
    </CActorModel>
