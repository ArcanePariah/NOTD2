<?xml version="1.0" encoding="us-ascii"?>
<Catalog>
    <CActorModel id="Blind" parent="ModelAnimationStyleContinuousUNIT">
        <Inherits index="CloakEffect" value="1"/>
        <Inherits index="Decal" value="1"/>
        <Inherits index="Visibility" value="1"/>
        <On Terms="ActorCreation" Send="AnimBracketStart GL_BSD GLbirth GLstand GLdeath"/>
        <On Terms="Behavior.Blind.On" Send="Create"/>
        <On Terms="ActorCreation" Send="AnimBracketStart $BSD"/>
        <On Terms="Behavior.Blind.Off" Send="AnimBracketStop BSD"/>
        <On Terms="ActorCreation" Send="SetTintColor {102,41,38 3.000000}"/>
        <Host Subject="_Unit"/>
        <HostSiteOps Ops="SOpAttachHead"/>
        <Model value="BlindImpact"/>
        <AutoScaleFromSelectionFactor value="1.000000"/>
    </CActorModel>
    <CActorModel id="OpenWound" parent="BuffContinuous">
        <On Terms="Behavior.OpenWound.On; ValidateUnitType IsBiological" Send="Create"/>
        <On Terms="ActorCreation" Send="$Birth"/>
        <On Terms="ActorCreation" Send="TimerSet 1.000000 loop"/>
        <On Terms="TimerExpired" Send="Create OpenWound"/>
        <On Terms="Behavior.OpenWound.Off" Send="Destroy"/>
        <Host Subject="_Unit"/>
        <HostSiteOps Ops="SOpAttachRandomDamage"/>
        <Model value="OpenWoundImpact"/>
        <AutoScaleFromSelectionFactor value="1.000000"/>
    </CActorModel>
