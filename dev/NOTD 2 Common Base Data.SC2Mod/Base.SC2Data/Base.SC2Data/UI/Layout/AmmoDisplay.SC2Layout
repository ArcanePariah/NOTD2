<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>

    <Frame type="Frame" name="AmmoDisplayTemplate">
        <Width val="200"/>
        <Height val="50"/>

        <Frame type="ProgressBar" name="AmmoBar">
            <Height val="20"/>
            <Width val="200"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="25"/>
            <ColorStep val="0,0,255"/>
            <MinValue val="0.000000"/>
            <Color val="0,0,0"/>
        </Frame>

        <Frame type="Label" name="AmmoCount">
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Height val="25"/>
            <Width val="100"/>
        </Frame>

        <Frame type="Button" name="HitTestFrame">
            <Height val="20"/>
            <Width val="200"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Top" relative="$parent" pos="Min" offset="25"/>
            <Shortcut val="M"/>
        </Frame>
    </Frame>
</Desc>
